# Ignore directories generated by Composer.
/vendor/

/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/

# Ignore sensitive information.
/web/sites/*/services.local.yml
/web/sites/*/settings.local.php

# Ignore Drupal's file directory.
/web/sites/*/files/

# Ignore operating system generated files.
.DS_Store

# Ignore other files and folders.
node_modules
/console/

# Ignore database dump files.
*.sql.gz
/lando/db-dump/*
!/lando/db-dump/.gitkeep

# Ignore .env files.
/.env
